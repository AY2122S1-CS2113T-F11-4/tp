@startuml
'https://plantuml.com/sequence-diagram
!include Style.puml

participant "CLIverShelf" as Main
participant ":Parser" as Parser
participant "newCommand: \nCommand" as Command



activate Main
Main -> Parser: parseCommand(input)
activate Parser
Parser -> Parser: prepareCommand(input)
activate Parser
Parser -> Command **: new Command(arguments)
deactivate Parser
activate Command
Command --> Parser: new Command
deactivate Command
Parser --> Main: new Command
deactivate Parser

Main -> Command: execute()
activate Command


Command --> Main
deactivate Command
deactivate Main


@enduml